<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">#FBCFCF</item>
        <item name="windowActionModeOverlay">true</item>
        <item name="colorPrimaryDark">#703C50</item>
        <item name="colorAccent">@color/colorAccent</item>
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>
    <style name="AppTheme.Dark" parent="Theme.AppCompat.DayNight.NoActionBar">
        <item name="actionMenuTextColor">#915454</item>
        <item name="windowActionModeOverlay">true</item>
        <item name="colorPrimary">#66DA00</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark_dark</item>
        <item name="colorAccent">@color/colorAccent_dark</item>
    </style>

    <style name="ToolBarStyle">ttt</style>

    />

</resources>
